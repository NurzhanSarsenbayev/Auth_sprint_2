# Django
DJANGO_SECRET_KEY=your-django-secret-key
DJANGO_DEBUG=0   # 0 for production, 1 for development

# Postgres
POSTGRES_DB=your-admin-db
POSTGRES_USER=your-db-user
POSTGRES_PASSWORD=your-db-password
POSTGRES_HOST=your-db-host
POSTGRES_PORT=5432

# Redis
REDIS_HOST=your-redis-host
REDIS_PORT=6379
CACHE_TTL=600

# Auth Service / JWKS
AUTH_JWKS_URL=http://your-auth-service:8000/api/v1/auth/.well-known/jwks.json
AUTH_ISS=http://your-auth-service:8000

# Admin access
ALLOWED_ADMIN_EMAILS=your-admin1@example.com,your-admin2@example.com
ADMIN_SUPERUSERS=your-admin1@example.com
